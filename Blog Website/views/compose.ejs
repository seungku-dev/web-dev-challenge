<%- include("partials/header"); -%>
      <h1>Compose</h1>
      <form action="/compose" method="post">
        <div class="mb-3">
          <label class="form-label" for="postTitle_01">Title</label>
          <input type="text" class="form-control" name="postTitle" id="postTitle_01" autocomplete="off">
        </div>
        <div class="mb-3">
          <label class="form-label" for="postBody_01">Post</label>
          <textarea class="form-control" name="postBody" id="postBody_01" rows="5" cols="30"></textarea>
        </div>
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
      </form>
<%- include("partials/footer"); -%>